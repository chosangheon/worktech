<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.groupware.worktech.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.groupware.worktech.board.model.vo.BoardFile" alias="BoardFile"/>
		<typeAlias type="com.groupware.worktech.admin.model.vo.Department" alias="Department"/>
		<typeAlias type="com.groupware.worktech.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.groupware.worktech.mail.model.vo.Mail" alias="Mail"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/admin-mapper.xml"/>
		<mapper resource="mappers/mail-mapper.xml"/>
	</mappers>
	
</configuration>
