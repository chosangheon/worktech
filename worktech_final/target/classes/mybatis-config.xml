<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.groupware.worktech.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.groupware.worktech.board.model.vo.BoardFile" alias="BoardFile"/>
		<typeAlias type="com.groupware.worktech.admin.model.vo.Department" alias="Department"/>
		<typeAlias type="com.groupware.worktech.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.groupware.worktech.mail.model.vo.Mail" alias="Mail"/>
		<typeAlias type="com.groupware.worktech.mail.model.vo.MailFile" alias="MailFile"/>
		<typeAlias type="com.groupware.worktech.model.vo.MailSR" alias="MailSR"/>

